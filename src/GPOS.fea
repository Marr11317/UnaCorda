#
# # GPOS
#
#
# # =================================================================================
# # LOOKUP DEFINITIONS
#
# 	# 1. vertical positioning -----------------------------------------------------
#
        lookup nudgeUp1 {
            lookupflag 0;
            pos @sepContent <0 100 0 0>;
            } nudgeUp1;

        lookup nudgeDown1 {
            lookupflag 0;
            pos @sepContent <0 -100 0 0>;
            } nudgeDown1;

        lookup nudgeDown2 {
            lookupflag 0;
            pos @sepContent <0 -200 0 0>;
            } nudgeDown2;


	# 2. horizontal positioning ---------------------------------------------------

		# 2.1 move to the left

			lookup back1 {
				lookupflag 0;
				pos @sepContent <-500 0 0 0>;
				} back1;

			lookup back2 {
				lookupflag 0;
				pos @parenContent        <-1000 0 0 0>;
				pos @paren0              <-1000 0 0 0>;
				pos @parenRoundEnlarged  < -930 0 0 0>;
				pos @parenSquareEnlarged < -890 0 0 0>;
				} back2;


		# 2.2 move to the right

			lookup forward1 {
				lookupflag 0;
				pos @sepContent <500 0 0 0>;
				} forward1;

			lookup forward2 {
				lookupflag 0;
				pos @parenContent        <1000 0 0 0>;
				pos @paren0              <1000 0 0 0>;
				pos @parenRoundEnlarged  < 930 0 0 0>;
				pos @parenSquareEnlarged < 890 0 0 0>;
				} forward2;



# =================================================================================
# FEATURES


	feature kern {

	# 1. CONTEXTUAL RULES ---------------------------------------------------------

		# 1.1 vertical position / layer

			lookup KERN_verticalPositionLayer2 {
				lookupflag 0;
				pos @sepContent' lookup layer2                                      @sep2 ;
				pos @sepContent' lookup layer2 @sepContent                          @sep2 ;
				pos @sepContent' lookup layer2 @sepContent @sepContent              @sep2 ;
				pos @sepContent' lookup layer2 @sepContent @sepContent @sepContent  @sep2 ;
				} KERN_verticalPositionLayer2;

			lookup KERN_verticalPositionLayer3 {
				lookupflag 0;
				pos @sepContent' lookup layer3                                      @sep3 ;
				pos @sepContent' lookup layer3 @sepContent                          @sep3 ;
				pos @sepContent' lookup layer3 @sepContent @sepContent              @sep3 ;
				pos @sepContent' lookup layer3 @sepContent @sepContent @sepContent  @sep3 ;
				} KERN_verticalPositionLayer3;

			lookup KERN_verticalPositionLayer4 {
				lookupflag 0;
				pos @sepContent' lookup layer4                                      @sep4 ;
				pos @sepContent' lookup layer4 @sepContent                          @sep4 ;
				pos @sepContent' lookup layer4 @sepContent @sepContent              @sep4 ;
				pos @sepContent' lookup layer4 @sepContent @sepContent @sepContent  @sep4 ;
				} KERN_verticalPositionLayer4;

			lookup KERN_verticalPositionLayer5 {
				lookupflag 0;
				pos @sepContent' lookup layer5                                      @sep5 ;
				pos @sepContent' lookup layer5 @sepContent                          @sep5 ;
				pos @sepContent' lookup layer5 @sepContent @sepContent              @sep5 ;
				pos @sepContent' lookup layer5 @sepContent @sepContent @sepContent  @sep5 ;
				} KERN_verticalPositionLayer5;


		# 1.2 horizontal position / column

			lookup KERN_horizontalPosition {
				lookupflag 0;

				pos                         @acc'        lookup back1      @figComp                            ;
				pos         @figComp        @acc'        lookup forward1                                       ;

				pos                         @parenLeft'  lookup back1                                  @sepPB  ;
				pos                         @parenLeft'  lookup back2                                  @sepPA  ;

				pos                         @parenLeft'  lookup back2      @parenContent @parenContent @sepPA  ;
				pos                         @parenLeft'  lookup back2      @acc @figComp                       ;
				pos                         @parenLeft'  lookup back2      @parenContent               @sepPA  ;
				pos                         @parenLeft'  lookup back1      @parenContent                       ;

				pos  @sepPE                 @parenRight' lookup forward2                                       ;
				pos  @sepPD                 @parenRight' lookup forward1                                       ;

				pos  @sepPE @acc @figComp   @parenRight' lookup forward2                                       ;
				pos  @sepPE @parenContent   @parenRight' lookup forward2                                       ;

				pos         @figComp @acc   @parenRight' lookup forward2                                       ;
				pos         @parenContent   @parenRight' lookup forward1                                       ;

				} KERN_horizontalPosition;


		# 1.3 kerning, collision avoidance

			# 1.3.1 fine tune horizontal position of modifying accidentals

				lookup KERN_spaceAroundTen {
					lookupflag 0;
					pos  @figCompTwoDigits  @acc' < 175 0 0 0>  ;
					pos  @acc' <-175 0 0 0>  @figCompTwoDigits  ;
					pos  @figCompTwoDigits  @acc  [parenright bracketright]' < 175 0 0 0>  ;
					pos  [parenleft bracketleft]' <-175 0 0 0>  @acc  @figCompTwoDigits  ;
					} KERN_spaceAroundTen;

				lookup KERN_modifyingAccidentalsRight {
					lookupflag 0;

					pos  @figComp                             n' < -50 0 0 0>  ;
				    pos  @figComp                             b' < -40 0 0 0>  ;
				    pos  @figComp                [numbersign s]' <   0 0 0 0>  ;
					pos  @figComp                           b_b' <  70 0 0 0>  ;
				    pos  @figComp                             x' < -10 0 0 0>  ;
				    pos  @figComp                             X' < -55 0 0 0>  ;
				    pos  @figComp                      [plus p]' <   0 0 0 0>  ;
				    pos  @figComp                    [hyphen m]' < -20 0 0 0>  ;

					pos  @figComp        parenleft_n_parenright' <195 0 0 0>  ;
					pos  @figComp        parenleft_b_parenright' <165 0 0 0>  ;
					pos  @figComp        parenleft_s_parenright' <205 0 0 0>  ;
					pos  @figComp      parenleft_b_b_parenright' <255 0 0 0>  ;
					pos  @figComp        parenleft_x_parenright' <180 0 0 0>  ;
					pos  @figComp        parenleft_p_parenright' <160 0 0 0>  ;
					pos  @figComp        parenleft_m_parenright' <160 0 0 0>  ;
					pos  @figComp        parenleft_X_parenright' <160 0 0 0>  ;

					pos  @figComp    bracketleft_n_bracketright' <210 0 0 0>  ;
					pos  @figComp    bracketleft_b_bracketright' <190 0 0 0>  ;
					pos  @figComp    bracketleft_s_bracketright' <205 0 0 0>  ;
					pos  @figComp  bracketleft_b_b_bracketright' <250 0 0 0>  ;
					pos  @figComp    bracketleft_x_bracketright' <185 0 0 0>  ;
					pos  @figComp    bracketleft_p_bracketright' <170 0 0 0>  ;
					pos  @figComp    bracketleft_m_bracketright' <150 0 0 0>  ;
					pos  @figComp    bracketleft_X_bracketright' <150 0 0 0>  ;

					} KERN_modifyingAccidentalsRight;


				lookup KERN_modifyingAccidentalsLeft {
					lookupflag 0;

					pos                             n' <  50 0 0 0>  @figComp  ;
				    pos                             b' <  40 0 0 0>  @figComp  ;
				    pos                [numbersign s]' <   0 0 0 0>  @figComp  ;
					pos                           b_b' < -60 0 0 0>  @figComp  ;
				    pos                             x' <  10 0 0 0>  @figComp  ;
				    pos                             X' <  55 0 0 0>  @figComp  ;
				    pos                      [plus p]' <   0 0 0 0>  @figComp  ;
				    pos                    [hyphen m]' <  20 0 0 0>  @figComp  ;

					pos        parenleft_n_parenright' <-195 0 0 0>  @figComp  ;
					pos        parenleft_b_parenright' <-165 0 0 0>  @figComp  ;
					pos        parenleft_s_parenright' <-205 0 0 0>  @figComp  ;
					pos      parenleft_b_b_parenright' <-295 0 0 0>  @figComp  ;
					pos        parenleft_x_parenright' <-180 0 0 0>  @figComp  ;
					pos        parenleft_p_parenright' <-160 0 0 0>  @figComp  ;
					pos        parenleft_m_parenright' <-150 0 0 0>  @figComp  ;
					pos        parenleft_X_parenright' <-150 0 0 0>  @figComp  ;

					pos    bracketleft_n_bracketright' <-210 0 0 0>  @figComp  ;
					pos    bracketleft_b_bracketright' <-180 0 0 0>  @figComp  ;
					pos    bracketleft_s_bracketright' <-195 0 0 0>  @figComp  ;
					pos  bracketleft_b_b_bracketright' <-280 0 0 0>  @figComp  ;
					pos    bracketleft_x_bracketright' <-185 0 0 0>  @figComp  ;
					pos    bracketleft_p_bracketright' <-170 0 0 0>  @figComp  ;
					pos    bracketleft_m_bracketright' <-160 0 0 0>  @figComp  ;
					pos    bracketleft_X_bracketright' <-160 0 0 0>  @figComp  ;

					} KERN_modifyingAccidentalsLeft;


				lookup KERN_parens {
					lookupflag 0;

					# parens around one
					pos                           [parenleft bracketleft]' <  60 0 0 0>  [one i_one]    ;
					pos            [one i_one]  [parenright bracketright]' < -60 0 0 0>                 ;

					# parens around a single number
					pos  @parenLeft' <50 0 0 0>  @figCompOneDigit  @parenRight               ;
					pos  @parenLeft              @figCompOneDigit  @parenRight' <-50 0 0 0>  ;

					# parens around numbers with two digits
					pos                       [parenleft bracketleft]' <-140 0 0 0>  @figCompTwoDigits  ;
					pos  @figCompTwoDigits  [parenright bracketright]' < 140 0 0 0>                     ;

					# parens around accidentals
					pos  [parenleft bracketleft]' <  90 0 0 0>  n               ;
					pos  [parenleft bracketleft]' <  80 0 0 0>  b               ;
					pos  [parenleft bracketleft]' <  40 0 0 0>  [numbersign s]  ;
					pos  [parenleft bracketleft]' <-100 0 0 0>  b_b             ;
					pos  [parenleft bracketleft]' <  90 0 0 0>  x               ;

					pos               n  [parenright bracketright]' < -90 0 0 0>  ;
					pos               b  [parenright bracketright]' <-140 0 0 0>  ;
					pos  [numbersign s]  [parenright bracketright]' < -40 0 0 0>  ;
					pos             b_b  [parenright bracketright]' <  40 0 0 0>  ;
					pos               x  [parenright bracketright]' < -90 0 0 0>  ;

					pos   parenleft.2' <120 0 0 0>   @figComp @acc   @sep   @acc @figComp   @sep   @figComp @acc   @parenRight   ;
					pos   parenleft.2' <120 0 0 0>   @parenContent   @sep   @acc @figComp   @sep   @figComp @acc   @parenRight   ;
					pos   parenleft.2' <120 0 0 0>   @figComp @acc   @sep   @acc @figComp   @sep   @parenContent   @parenRight   ;
					pos   parenleft.2' <120 0 0 0>   @parenContent   @sep   @acc @figComp   @sep   @parenContent   @parenRight   ;

					pos   @parenLeft   @acc @figComp   @sep   @figComp @acc   @sep   @acc @figComp   parenright.2' <-120 0 0 0>  ;
					pos   @parenLeft   @parenContent   @sep   @figComp @acc   @sep   @acc @figComp   parenright.2' <-120 0 0 0>  ;
					pos   @parenLeft   @acc @figComp   @sep   @figComp @acc   @sep   @parenContent   parenright.2' <-120 0 0 0>  ;
					pos   @parenLeft   @parenContent   @sep   @figComp @acc   @sep   @parenContent   parenright.2' <-120 0 0 0>  ;

					# parens around modified numbers

					ignore pos   @sepPE  [two_plus i_two_plus four_plus i_four_plus four_tick i_four_tick five_plus i_five_plus]  @parenRight' ;

					# two_plus
					pos    two_plus  @parenRight' <330 0 0 0>  ;

					pos    two_plus  @sepPD  @parenContent  parenright.2'  <170 0 0 0>  ;
					pos    two_plus  @sepPD  @acc @figComp  parenright.2'  <170 0 0 0>  ;

					pos    two_plus  @sepPD  @acc @figComp  @parenRoundRight'  <280 0 0 0>  ;
					pos    two_plus  @sepPD  @acc @figComp  @parenSquareRight' <330 0 0 0>  ;
					pos    two_plus  @sepPD  @parenContent  @parenRoundRight'  <280 0 0 0>  ;
					pos    two_plus  @sepPD  @parenContent  @parenSquareRight' <330 0 0 0>  ;

					pos    two_plus  @sep  @acc @figComp  @sepPD  @acc @figComp  @parenRoundRight'  <250 0 0 0>  ;
					pos    two_plus  @sep  @acc @figComp  @sepPD  @acc @figComp  @parenSquareRight' <330 0 0 0>  ;
					pos    two_plus  @sep  @acc @figComp  @sepPD  @parenContent  @parenRoundRight'  <250 0 0 0>  ;
					pos    two_plus  @sep  @acc @figComp  @sepPD  @parenContent  @parenSquareRight' <330 0 0 0>  ;
					pos    two_plus  @sep  @parenContent  @sepPD  @acc @figComp  @parenRoundRight'  <250 0 0 0>  ;
					pos    two_plus  @sep  @parenContent  @sepPD  @acc @figComp  @parenSquareRight' <330 0 0 0>  ;
					pos    two_plus  @sep  @parenContent  @sepPD  @parenContent  @parenRoundRight'  <250 0 0 0>  ;
					pos    two_plus  @sep  @parenContent  @sepPD  @parenContent  @parenSquareRight' <330 0 0 0>  ;

					# two_tick
					pos    @sepPD  [two_tick two_tick.alt i_two_tick i_two_tick.alt]  @parenRoundRight'  <60 0 0 0>  ;
					pos    @sepPD  [two_tick two_tick.alt i_two_tick i_two_tick.alt]  @parenSquareRight' <30 0 0 0>  ;

					# i_two_plus
					pos    i_two_plus  @parenRight' <280 0 0 0>  ;

					pos    i_two_plus  @sepPD  @parenContent  parenright.2'  <120 0 0 0>  ;
					pos    i_two_plus  @sepPD  @acc @figComp  parenright.2'  <120 0 0 0>  ;

					pos    i_two_plus  @sepPD  @acc @figComp  @parenRoundRight'  <230 0 0 0>  ;
					pos    i_two_plus  @sepPD  @acc @figComp  @parenSquareRight' <280 0 0 0>  ;
					pos    i_two_plus  @sepPD  @parenContent  @parenRoundRight'  <230 0 0 0>  ;
					pos    i_two_plus  @sepPD  @parenContent  @parenSquareRight' <280 0 0 0>  ;

					pos    i_two_plus  @sep  @acc @figComp  @sepPD  @acc @figComp  @parenRoundRight'  <200 0 0 0>  ;
					pos    i_two_plus  @sep  @acc @figComp  @sepPD  @acc @figComp  @parenSquareRight' <280 0 0 0>  ;
					pos    i_two_plus  @sep  @acc @figComp  @sepPD  @parenContent  @parenRoundRight'  <200 0 0 0>  ;
					pos    i_two_plus  @sep  @acc @figComp  @sepPD  @parenContent  @parenSquareRight' <280 0 0 0>  ;
					pos    i_two_plus  @sep  @parenContent  @sepPD  @acc @figComp  @parenRoundRight'  <200 0 0 0>  ;
					pos    i_two_plus  @sep  @parenContent  @sepPD  @acc @figComp  @parenSquareRight' <280 0 0 0>  ;
					pos    i_two_plus  @sep  @parenContent  @sepPD  @parenContent  @parenRoundRight'  <200 0 0 0>  ;
					pos    i_two_plus  @sep  @parenContent  @sepPD  @parenContent  @parenSquareRight' <280 0 0 0>  ;

					# four_plus, i_four_plus
					pos    [four_plus i_four_plus]  @parenRight' <300 0 0 0>  ;

					pos    [four_plus i_four_plus]  @sepPD  @parenContent  parenright.2'  <140 0 0 0>  ;
					pos    [four_plus i_four_plus]  @sepPD  @acc @figComp  parenright.2'  <140 0 0 0>  ;

					pos    [four_plus i_four_plus]  @sepPD  @acc @figComp  @parenRoundRight'  <250 0 0 0>  ;
					pos    [four_plus i_four_plus]  @sepPD  @acc @figComp  @parenSquareRight' <300 0 0 0>  ;
					pos    [four_plus i_four_plus]  @sepPD  @parenContent  @parenRoundRight'  <250 0 0 0>  ;
					pos    [four_plus i_four_plus]  @sepPD  @parenContent  @parenSquareRight' <300 0 0 0>  ;

					pos    [four_plus i_four_plus]  @sep  @acc @figComp  @sepPD  @acc @figComp  @parenRoundRight'  <220 0 0 0>  ;
					pos    [four_plus i_four_plus]  @sep  @acc @figComp  @sepPD  @acc @figComp  @parenSquareRight' <300 0 0 0>  ;
					pos    [four_plus i_four_plus]  @sep  @acc @figComp  @sepPD  @parenContent  @parenRoundRight'  <220 0 0 0>  ;
					pos    [four_plus i_four_plus]  @sep  @acc @figComp  @sepPD  @parenContent  @parenSquareRight' <300 0 0 0>  ;
					pos    [four_plus i_four_plus]  @sep  @parenContent  @sepPD  @acc @figComp  @parenRoundRight'  <220 0 0 0>  ;
					pos    [four_plus i_four_plus]  @sep  @parenContent  @sepPD  @acc @figComp  @parenSquareRight' <300 0 0 0>  ;
					pos    [four_plus i_four_plus]  @sep  @parenContent  @sepPD  @parenContent  @parenRoundRight'  <220 0 0 0>  ;
					pos    [four_plus i_four_plus]  @sep  @parenContent  @sepPD  @parenContent  @parenSquareRight' <300 0 0 0>  ;

					# five_plus
					pos    five_plus  @parenRight' <250 0 0 0>  ;

					pos    five_plus  @sepPD  @parenContent  parenright.2'  <90 0 0 0>  ;
					pos    five_plus  @sepPD  @acc @figComp  parenright.2'  <90 0 0 0>  ;

					pos    five_plus  @sepPD  @acc @figComp  @parenRoundRight'  <200 0 0 0>  ;
					pos    five_plus  @sepPD  @acc @figComp  @parenSquareRight' <250 0 0 0>  ;
					pos    five_plus  @sepPD  @parenContent  @parenRoundRight'  <200 0 0 0>  ;
					pos    five_plus  @sepPD  @parenContent  @parenSquareRight' <250 0 0 0>  ;

					pos    five_plus  @sep  @acc @figComp  @sepPD  @acc @figComp  @parenRoundRight'  <170 0 0 0>  ;
					pos    five_plus  @sep  @acc @figComp  @sepPD  @acc @figComp  @parenSquareRight' <250 0 0 0>  ;
					pos    five_plus  @sep  @acc @figComp  @sepPD  @parenContent  @parenRoundRight'  <170 0 0 0>  ;
					pos    five_plus  @sep  @acc @figComp  @sepPD  @parenContent  @parenSquareRight' <250 0 0 0>  ;
					pos    five_plus  @sep  @parenContent  @sepPD  @acc @figComp  @parenRoundRight'  <170 0 0 0>  ;
					pos    five_plus  @sep  @parenContent  @sepPD  @acc @figComp  @parenSquareRight' <250 0 0 0>  ;
					pos    five_plus  @sep  @parenContent  @sepPD  @parenContent  @parenRoundRight'  <170 0 0 0>  ;
					pos    five_plus  @sep  @parenContent  @sepPD  @parenContent  @parenSquareRight' <250 0 0 0>  ;

					# i_five_plus
					pos    i_five_plus  @parenRight' <300 0 0 0>  ;

					pos    i_five_plus  @sepPD  @parenContent  parenright.2'  <140 0 0 0>  ;
					pos    i_five_plus  @sepPD  @acc @figComp  parenright.2'  <140 0 0 0>  ;

					pos    i_five_plus  @sepPD  @acc @figComp  @parenRoundRight'  <250 0 0 0>  ;
					pos    i_five_plus  @sepPD  @acc @figComp  @parenSquareRight' <300 0 0 0>  ;
					pos    i_five_plus  @sepPD  @parenContent  @parenRoundRight'  <250 0 0 0>  ;
					pos    i_five_plus  @sepPD  @parenContent  @parenSquareRight' <300 0 0 0>  ;

					pos    i_five_plus  @sep  @acc @figComp  @sepPD  @acc @figComp  @parenRoundRight'  <220 0 0 0>  ;
					pos    i_five_plus  @sep  @acc @figComp  @sepPD  @acc @figComp  @parenSquareRight' <300 0 0 0>  ;
					pos    i_five_plus  @sep  @acc @figComp  @sepPD  @parenContent  @parenRoundRight'  <220 0 0 0>  ;
					pos    i_five_plus  @sep  @acc @figComp  @sepPD  @parenContent  @parenSquareRight' <300 0 0 0>  ;
					pos    i_five_plus  @sep  @parenContent  @sepPD  @acc @figComp  @parenRoundRight'  <220 0 0 0>  ;
					pos    i_five_plus  @sep  @parenContent  @sepPD  @acc @figComp  @parenSquareRight' <300 0 0 0>  ;
					pos    i_five_plus  @sep  @parenContent  @sepPD  @parenContent  @parenRoundRight'  <220 0 0 0>  ;
					pos    i_five_plus  @sep  @parenContent  @sepPD  @parenContent  @parenSquareRight' <300 0 0 0>  ;

					# four_tick
					pos    [four_tick i_four_tick]  @parenRight' <200 0 0 0>  ;

					pos    [four_tick i_four_tick]  @sepPD  @parenContent  parenright.2'  <40 0 0 0>  ;
					pos    [four_tick i_four_tick]  @sepPD  @acc @figComp  parenright.2'  <40 0 0 0>  ;

					pos    [four_tick i_four_tick]  @sepPD  @acc @figComp  @parenRoundRight'  <150 0 0 0>  ;
					pos    [four_tick i_four_tick]  @sepPD  @acc @figComp  @parenSquareRight' <200 0 0 0>  ;
					pos    [four_tick i_four_tick]  @sepPD  @parenContent  @parenRoundRight'  <150 0 0 0>  ;
					pos    [four_tick i_four_tick]  @sepPD  @parenContent  @parenSquareRight' <200 0 0 0>  ;

					pos    [four_tick i_four_tick]  @sep  @acc @figComp  @sepPD  @acc @figComp  @parenRoundRight'  <120 0 0 0>  ;
					pos    [four_tick i_four_tick]  @sep  @acc @figComp  @sepPD  @acc @figComp  @parenSquareRight' <200 0 0 0>  ;
					pos    [four_tick i_four_tick]  @sep  @acc @figComp  @sepPD  @parenContent  @parenRoundRight'  <120 0 0 0>  ;
					pos    [four_tick i_four_tick]  @sep  @acc @figComp  @sepPD  @parenContent  @parenSquareRight' <200 0 0 0>  ;
					pos    [four_tick i_four_tick]  @sep  @parenContent  @sepPD  @acc @figComp  @parenRoundRight'  <120 0 0 0>  ;
					pos    [four_tick i_four_tick]  @sep  @parenContent  @sepPD  @acc @figComp  @parenSquareRight' <200 0 0 0>  ;
					pos    [four_tick i_four_tick]  @sep  @parenContent  @sepPD  @parenContent  @parenRoundRight'  <120 0 0 0>  ;
					pos    [four_tick i_four_tick]  @sep  @parenContent  @sepPD  @parenContent  @parenSquareRight' <200 0 0 0>  ;

					} KERN_parens;

			# 1.3.2 single accidentals

				lookup KERN_singleAccidentals {
					lookupflag 0;
					pos  @sepContent  @sep             @accSingleDown' lookup nudgeDown1  @parenAll  @sep  ;
					pos  @sepContent  @sep  @parenAll  @accSingleDown' lookup nudgeDown1             @sep  ;
					pos  @sepContent  @sep             @accSingleDown' lookup nudgeDown1             @sep  ;

					pos  @sepContent  @sep             @accSingleUp'   lookup nudgeUp1    @parenAll  @sep  ;
					pos  @sepContent  @sep  @parenAll  @accSingleUp'   lookup nudgeUp1               @sep  ;
					pos  @sepContent  @sep             @accSingleUp'   lookup nudgeUp1               @sep  ;

					ignore pos  @figComp  @accSingleDown'                               @sep               ;
					ignore pos            @accSingleDown'                    @figComp   @sep               ;
					pos                   @accSingleDown' lookup nudgeUp1               @sep  @sepContent  ;


					pos  @sepContent  @sep             @flat  @parenAll  @sep                                                                                                                                                            @allNoParens' lookup nudgeDown1  ;
					pos  @sepContent  @sep  @parenAll  @flat             @sep                                                                                                                                                            @allNoParens' lookup nudgeDown1  ;
